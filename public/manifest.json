{
  "manifest_version": 3,
  "name": "PRD For AI",
  "version": "1.0.0",
  "description": "AI Coding的需求扩写小帮手",
  "permissions": [
    "activeTab",
    "sidePanel",
    "storage"
  ],
  "host_permissions": [
    "http://localhost:8001/*",
    "http://teach.excelmaster.ai/*",
    "https://teach.excelmaster.ai/*"
  ],
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "action": {
    "default_icon": {
      "128": "image.png"
    },
    "default_title": "打开 PRD For AI"
  },
  "icons": {
    "128": "image.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "**/*"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}



